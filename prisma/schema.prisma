// schema.prisma

generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider = "mysql"
    url      = env("DATABASE_URL")
}

model Employee {
    patientNumber   Int    @id @default(autoincrement())
    patientName     String
    email           String
    phoneNumber     String
    patientPassword String
    birthday        String
    address         String
    license         String
    gender          String
}
